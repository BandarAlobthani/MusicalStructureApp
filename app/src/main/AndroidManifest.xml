<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnglishSong"
            android:label="@string/category_English_Song"/>/>
        <activity android:name=".ArabicSong"
            android:label="@string/category_Arabic_Song"/>
        <activity android:name=".LatinSong"
            android:label="@string/category_Latin_Song"/>
        <activity android:name=".FrenchSong"
            android:label="@string/category_French_Song"></activity>
    </application>

</manifest>